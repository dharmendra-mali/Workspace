package Example;

import java.util.ArrayDeque;
import java.util.BitSet;
import java.util.Scanner;

public class Silution {

	private static Scanner sc;

	public static void main(String[] args) {
        
		BitSet bs1 = new BitSet();
		
		for(int i=0;i<5;i++){
			bs1.set(i,0);	
		}
		System.out.println(bs1);
        
	}

}
