import java.util.ArrayList;
import java.util.List;

public class ListExample {

	public static void main(String[] args) {

		List<Integer> list = new ArrayList<>();

		for (int i = 1; i < 11; i++) {
			list.add(i);
		}

		list.stream().filter(i->i==1);
		list.forEach(add -> System.out.print(add+" "));
	}

}
